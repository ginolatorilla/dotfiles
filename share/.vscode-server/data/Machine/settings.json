{
    "aws.profile": "profile:default",
    "editor.fontFamily": "'Fira Code Retina', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [79, 99, 119],
    "editor.smoothScrolling": true,
    "editor.suggestSelection": "first",
    "files.autoSave": "off",
    "git.autofetch": true,
    "mesonbuild.configureOnOpen": true,
    "omnisharp.defaultLaunchSolution": "",
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.useGlobalMono": "never",
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "python.autoComplete.addBrackets": true,
    "python.diagnostics.sourceMapsEnabled": false,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfPath": "~/.local/bin/yapf",
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "~/.local/bin/flake8",
    "python.linting.mypyEnabled": true,
    "python.linting.mypyPath": "~/.local/bin/mypy",
    "python.testing.pytestEnabled": true,
    "terminal.integrated.detectLocale": "off",
    "testExplorer.codeLens": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Atom One Light"
}