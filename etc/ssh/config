Include ~/work/.sshconfig
Include ~/projects/.sshconfig

Host gitlab.com
    Hostname altssh.gitlab.com
    Port 443
    User git
    PreferredAuthentications publickey

Host *
    User ginolatorilla
    ForwardAgent yes
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_rsa.ginolatorilla
    ServerAliveInterval 60
